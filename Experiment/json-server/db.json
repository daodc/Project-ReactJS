    "data": { "items": [{ "@id": 1, "id": 34, "tieuDe": "VBSP Đà Nẵng ký kết hợp tác với Nayoby Bank Savannakhet", "tomTat": "Tin tức Đà Nẵng Ngày 30/9/2014, tại TP. Đà Nẵng, Ngân hàng chính sách xã hội Việt Nam (VBSP) đã tổ chức lễ ký kết biên bản ghi nhớ về kết nghĩa giữa VBSP chi nhánh TP. Đà Nẵng và Ngân hàng chính sách Lào (Nayoby Bank) chi nhánh tỉnh Savannakhet.", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 2, "id": 438, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/542b85b32e9f2_medium_1412218040136_1412924447905.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_542b85b32e9f2_medium_1412218040136_1412924447905.jpg" }, "chuDe": { "@id": 3, "id": 6, "ten": "Tổng hợp", "moTa": "Tổng hợp", "alias": "tong-hop", "lopChuDe": 6, "anhDaiDien": null, "thuTu": 1 }, "khuVucKhuyenMai": null, "view": 224, "diaDiem": null, "interest": 1, "comment": 0, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "02/10/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "vbsp-da-nang-ky-ket-hop-tac-voi-nayoby-bank-savannakhet-34", "tenChuDe": "Tổng hợp" }, { "@id": 4, "id": 18, "tieuDe": "Đường đẹp nhất Đà Nẵng mang tên Đại tướng Võ Nguyên Giáp ", "tomTat": "Nằm giữa đường Hoàng Sa và Trường Sa, đường Võ Nguyên Giáp được được đánh giá là đẹp nhất thành phố ven Sông Hàn.", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 5, "id": 97, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/IMG-9225-9941-1387360460-435x305_1411197047714.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_IMG-9225-9941-1387360460-435x305_1411197047714.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 146, "diaDiem": null, "interest": 1, "comment": 4, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "duong-dep-nhat-da-nang-mang-ten-dai-tuong-vo-nguyen-giap--18", "tenChuDe": "Tổng hợp" }, { "@id": 6, "id": 17, "tieuDe": "Về giọng nói nơi không có xe Lam", "tomTat": "Xưa nay Quảng Nam có lẽ là địa phương mà giọng nói bị đem ra trêu ghẹo nhiều nhất nước. Nói cho công bằng, so với một số vùng miền Bắc và miền Nam, người Quảng phát âm rất chuẩn xác các phụ âm đầu.", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 7, "id": 76, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/xe-lam-quang-nam_1411196784169.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_xe-lam-quang-nam_1411196784169.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 100, "diaDiem": null, "interest": 0, "comment": 1, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "ve-giong-noi-noi-khong-co-xe-lam-17", "tenChuDe": "Tổng hợp" }, { "@id": 8, "id": 16, "tieuDe": "Du lịch Đà Nẵng: Thanh bình với Sơn Trà Tịnh Viên", "tomTat": "Có một Sơn Trả Tịnh Viên giữa rừng Sơn Trả Đà Nẵng , cách xa bao ồn ào , xô bồ của chốn thành thị để con người tìm về chốn yên bình", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 9, "id": 74, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/12_1411196504232.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_12_1411196504232.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 103, "diaDiem": null, "interest": 1, "comment": 2, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "du-lich-da-nang-thanh-binh-voi-son-tra-tinh-vien-16", "tenChuDe": "Tổng hợp" }, { "@id": 10, "id": 15, "tieuDe": "Khám phá Đà Nẵng: Thành phố trong những năm 70", "tomTat": "Khám phá Đà Nẵng - Đường phố buổi trưa, nhà hàng nổi trên sông, trẻ em xếp hàng vào lớp những hình ảnh sống động về Đà Nẵng năm 1970.", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 11, "id": 73, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/Redsvn-Da-Nang-1970-Steve-Ferendo-08_1411196370115.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_Redsvn-Da-Nang-1970-Steve-Ferendo-08_1411196370115.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 101, "diaDiem": null, "interest": 0, "comment": 1, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "thanh-pho-trong-nhung-nam-70-15", "tenChuDe": "Tổng hợp" }, { "@id": 12, "id": 14, "tieuDe": "Du lịch Đà Nẵng: Khám phá bảo tàng Đồng Đình", "tomTat": "Du lịch Đà Nẵng - Đà Nẵng là một thành phố du lịch cực kỳ nổi tiếng với rất nhiều điểm đến hấp dẫn, và trong chuyến hành trình của bạn chắc chắn không thể bỏ qua một địa danh không mới nhưng lại được mọi người nhắc đến khá nhiều, đó là bảo tàng Đồng Đình nằm ở bãi Bụt.\n", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 13, "id": 63, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/Picture_019_1411196202125.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_Picture_019_1411196202125.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 81, "diaDiem": null, "interest": 0, "comment": 0, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "kham-pha-bao-tang-dong-dinh-14", "tenChuDe": "Tổng hợp" }, { "@id": 14, "id": 13, "tieuDe": "Du lịch Đà Nẵng: Hùng vỹ hầm đèo Hải Vân", "tomTat": "Du lịch Đà Nẵng - Trên con đường xuyên Việt qua dãi đất miền Trung đầy nắng gió, từ lâu hầm đèo Hải Vân - Đà Nẵng đã nổi tiếng là đường đèo đẹp nhất và cũng hiểm trở nhất Việt Nam.\n", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 15, "id": 62, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/deo-hai-van_1411196062483.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_deo-hai-van_1411196062483.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 18, "diaDiem": null, "interest": 0, "comment": 0, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "hung-vy-ham-deo-hai-van-13", "tenChuDe": "Tổng hợp" }, { "@id": 16, "id": 12, "tieuDe": "Sáng tạo robot dành cho học sinh THPT Đà nẵng 2014", "tomTat": "Tin tức Đà Nẵng -  Cuộc thi “Sáng tạo Robot dành cho học sinh TP Đà Nẵng 2014” sẽ diễn ra từ 12 - 14/8. 63 chú Robot đến từ 21 đội của các trường THPT trên địa bàn thành phố Đà Nẵng và Quảng Nam đã được các đội tất bật gấp rút hoàn thiện.\n", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 17, "id": 49, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/1_1_quwa_1411195683833.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_1_1_quwa_1411195683833.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 63, "diaDiem": null, "interest": 0, "comment": 0, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "sang-tao-robot-danh-cho-hoc-sinh-thpt-da-nang-2014-12", "tenChuDe": "Tổng hợp" }, { "@id": 18, "id": 11, "tieuDe": "Từ Hải Vân Quan xuôi về Ngũ Hành Sơn", "tomTat": "Với những thuận lợi về vị trí địa lý, khí hậu, Hải Vân Quan được xem là điểm dừng chân lý tưởng của du khách.\n", "noiDung": null, "tacGia": null, "thuTu": 1, "anhDaiDien": { "@id": 19, "id": 48, "urlImage": "http://backend.yourdanang.vn/mis/baiviet/NHS_1411195513866.jpg", "urlThumbImage": "http://backend.yourdanang.vn/mis/baiviet/thumb_NHS_1411195513866.jpg" }, "chuDe": 3, "khuVucKhuyenMai": null, "view": 45, "diaDiem": null, "interest": 1, "comment": 0, "lopChuDe": 6, "alias": "tong-hop", "ngayBatDauXuatBanStr": "20/09/2014", "ngayBatDauKhuyenMaiStr": "", "ngayKetThucKhuyenMaiStr": "", "thoiGianKetThucStr": "", "thoiGianDienRaStr": "", "aliasBaiViet": "tu-hai-van-quan-xuoi-ve-ngu-hanh-son-11", "tenChuDe": "Tổng hợp" }], "total": 100, "page": 1, "limit": 15 }
